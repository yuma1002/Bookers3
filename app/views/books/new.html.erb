<h1>NewBook</h1>

<%= form_with model: @book, url: '/books', method: :post do |f| %>
<h4>Title</h4>
<%= f.text_field :title %>

<h4>Body</h4>
<%= f.text_area :body %>
<%= f.submit 'Create Book' %>
<% end %>


